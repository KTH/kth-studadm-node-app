{
  "name": "@kth/kth-studadm-node-app",
  "version": "4.0.0",
  "description": "Provides common functionality for the StudAdm node applications",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "automaticPublish": "true",
  "scripts": {
    "build": "tsc",
    "start": "node dist/example/app.js",
    "watch-client": "webpack --config src/example/webpack.config.js --watch",
    "codecheck": "tslint -p tsconfig.json -c tslint.json 'src/**/*.{ts,tsx}'",
    "codecheck-fix": "tslint -c tslint.json --fix 'src/**/*.{ts,tsx}'",
    "preversion": "npm run codecheck && npm test",
    "postversion": "git push && git push --tags",
    "prepack": "rm -r dist && npm run build && npm test && npm run codecheck",
    "test": "mocha --require ts-node/register \"src/**/*.test.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KTH/kth-studadm-node-app.git"
  },
  "author": {
    "name": "Stefan Sundkvist",
    "email": "sst@kth.se"
  },
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/node": "^10.12.10",
    "@types/redis": "^2.8.8",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "inferno": "^5.2.0",
    "inferno-create-element": "^5.2.0",
    "inferno-server": "^5.1.1",
    "kth-node-cortina-block": "^3.1.0",
    "kth-node-redis": "^1.2.7",
    "kth-node-web-common": "^5.1.3",
    "kth-style": "^5.0.18",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cookie-session": "^2.0.36",
    "@types/mocha": "^5.2.5",
    "@types/passport": "^0.4.7",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "^4.2.0",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^6.1.0",
    "kth-node-configuration": "^1.6.4",
    "kth-node-ldap": "^2.3.8",
    "kth-node-log": "^3.0.2",
    "kth-node-passport-cas": "^3.1.7",
    "mocha": "^5.2.0",
    "passport": "^0.4.0",
    "ts-node": "^9.1.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.1",
    "webpack-assets-manifest": "^3.0.1",
    "webpack-cli": "^3.0.8"
  },
  "peerDependencies": {
    "passport": "^0.4.0",
    "kth-node-log": "^3.0.2",
    "kth-node-passport-cas": "^3.1.3"
  }
}
